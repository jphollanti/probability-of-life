{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }, 
  "rules": {
    "interface-name": [true, "never-prefix"],
    "ordered-imports": [
      false,
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "object-literal-sort-keys": false,
    "comment-format": [false],
    "jsx-no-lambda": false
  }
}
